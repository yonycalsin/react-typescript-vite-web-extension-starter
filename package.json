    "@vitejs/plugin-react": "1.3.2",
    "@yonycalsin/prettier-config-react-typescript": "1.0.1",
    "prettier": "2.6.2",
    "prettier-plugin-packagejson": "2.2.18",
    "vite": "2.9.9"
  },
  "volta": {
    "node": "16.14.0"
  },
  "dependencies": {
    "react": "18.1.0",
